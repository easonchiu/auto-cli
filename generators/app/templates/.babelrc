{
    "presets": [
        "react",
    	["env", {
            "targets": {
                "browsers": ["ios >= 7", "android >= 4.4"]
            }
        }]
    ],
    "plugins": [
        "lodash",
    	"transform-runtime",
        "transform-object-rest-spread",
    	"transform-decorators-legacy",
        "syntax-dynamic-import",
    	["component", [
    		{
    			libraryName: "auto"
    		}
    	]]
    ]
}